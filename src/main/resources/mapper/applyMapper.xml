<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.test.mapper.applymapper">

  <select id="getApply" parameterType="String" resultType="com.example.test.bean.applybean">
    SELECT * FROM apply WHERE phonenum = #{phonenum} AND ip = #{ip} AND deviceid = #{deviceid}
  </select>
  <insert id="addApply" parameterType="com.example.test.bean.applybean">
    INSERT INTO apply VALUES(#{phonenum},#{ip},#{deviceid},#{code},#{expiretime})
  </insert>
</mapper>